$synonyms = array("QR code","bank account","net banking","online banking","internet banking","netbanking", "fastag", "fas tag", "fednet", "fed net", "reward points", "debit card", "3d password", "otp","credit card", "cc");




function powerSet($in,$subqu,$synonyms,$minLength = 1) { 
   $count = count($in); 
   $members = pow(2,$count); 
   $return = array(); 


   for ($i = 0; $i < $members; $i++) { 
      $b = sprintf("%0".$count."b",$i); 


      $out = array(); 
      for ($j = 0; $j < $count; $j++) { 
         if ($b{$j} == '1') $out[] = $in[$j]; 


      }
      $out_val = implode(" ", $out); 
      if (count($out) >= $minLength) { 
         $return[] = $out_val; 
         
      } 
   } 


		$result=array_intersect($return,$synonyms);

		if(!empty($result))
		{

		foreach ($result as $key => $value) {
			$replace[]='{'.$value.'}';

		}
		  $newPhrase = str_replace($result, $replace, $subqu);

		}
		else
		{
			 $newPhrase = $subqu;
		}
		return $newPhrase;





}






	foreach ($sub_questions as $key => $sub) {

		$subqu=$sub['sub_question'];

		$text = preg_replace("/\r|\n/", "", $subqu);

		$sub_question = preg_replace('/[^A-Za-z0-9 ]/', '', $text);


		 $words1 =explode(" ",$sub_question);
		 $words= array_values(array_filter($words1));

		 $num_words = count($words);

		 $word_comb=powerSet($words,$subqu,$synonyms);



		  echo $word_comb;

		  echo "<br>";




}



